  "paths": {
    "/insert/{data}": {
      "get": {
        "summary": "Insert Data",
        "description": "Stores received data to a log file.",
        "parameters": [
          {
            "name": "data",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string" },
                    "inserted_data": { "type": "string" },
                    "timestamp": { "type": "string" },
                    "file": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/read/1": {
      "get": {
        "summary": "Read Sample Response 1",
        "description": "Returns sample JSON 1.",
